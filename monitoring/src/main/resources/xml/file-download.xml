<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Script xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schema.xsd">
    <applicationURL>https://a5.adstream.com</applicationURL>
    <action>
        <method>authSSO</method>
        <argument>
            <type>String</type>
            <value>monitoring@adstream.com</value>
        </argument>
        <argument>
            <type>String</type>
            <value>6Welcome!</value>
        </argument>
    </action>
    <action>
        <method>downloadFile</method>
        <argument>
            <type>String</type>
            <!-- a5 file id -->
            <value>51f9024ae4b0d99fd28bebad</value>
        </argument>
        <argument>
            <type>String</type>
            <!-- master file id -->
            <value>de24c9e9-cf56-4b12-bcf9-a20c00dd5086</value>
        </argument>
        <argument>
            <type>String</type>
            <!-- save as -->
            <value>monitoring_file.jpg</value>
        </argument>
        <assertion>
            <!-- file size in bytes -->
            <size>226267</size>
        </assertion>
    </action>
    <action>
        <method>logoutSSO</method>
    </action>
</Script>