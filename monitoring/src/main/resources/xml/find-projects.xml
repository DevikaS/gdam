<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Script xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schema.xsd">
    <applicationURL>https://a5.adstream.com</applicationURL>
    <action>
        <method>authSSO</method>
        <argument>
            <type>String</type>
            <value>monitoring@adstream.com</value>
        </argument>
        <argument>
            <type>String</type>
            <value>6Welcome!</value>
        </argument>
    </action>
    <action>
        <method>findProjects</method>
        <argument>
            <type>Search</type>
            <searchValue>
                <query>_cm.common.name:monitoring_project</query>
            </searchValue>
        </argument>
        <assertion>
            <sizeMin>1</sizeMin>
        </assertion>
    </action>
    <action>
        <method>getProject</method>
        <argument>
            <type>String</type>
            <value>51f90159e4b0d99fd28beba3</value>
        </argument>
        <assertion>
            <name>monitoring_project</name>
        </assertion>
    </action>
    <action>
        <method>logoutSSO</method>
